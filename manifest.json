{
  "manifest_version": 2,
  "name": "Exquisite Pepes",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "pepe.png"
  }
}